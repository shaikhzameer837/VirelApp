<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:text="Support help"
        android:textStyle="bold"
        android:layout_margin="5dp"
        android:paddingLeft="15dp"
        android:gravity="center_vertical"
        android:background="#dddddd"
        android:textColor="#000000"
        android:layout_height="45dp"/>
    <TextView android:id="@+id/discord"
        android:layout_width="match_parent"
        android:text=" Discord"
        android:gravity="center_vertical"
        android:drawableLeft="@drawable/discord"
        android:padding="15dp"
        android:layout_height="wrap_content"/>
    <View
        android:layout_width="match_parent"
        android:background="#dddddd"
        android:layout_height="1dp"/>
   <TextView
        android:layout_width="match_parent"
        android:text=" Instagran"
       android:id="@+id/instagram"
        android:drawableLeft="@drawable/instagram"
        android:padding="15dp"
       android:gravity="center_vertical"
       android:layout_height="wrap_content"/>
    <View
        android:layout_width="match_parent"
        android:background="#dddddd"
        android:layout_height="1dp"/>
   <TextView
        android:layout_width="match_parent"
        android:text=" Youtube"
       android:id="@+id/youtube"
        android:drawableLeft="@drawable/youtube"
        android:padding="15dp"
       android:gravity="center_vertical"
       android:layout_height="wrap_content"/>
    <View
        android:layout_width="match_parent"
        android:background="#dddddd"
        android:layout_height="1dp"/>
</LinearLayout>